{
    "$id": "http://example.com/example.json",
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "username": {
        "$id": "/properties/username",
        "type": "string",
        "title": "The Username Schema ",
        "default": "",
        "minLength": 1,
        "maxLength": 55,
        "examples": ["hueter"]
      },
      "password": {
        "$id": "/properties/password",
        "type": "string",
        "title": "The Password Schema ",
        "default": "",
        "minLength": 5,
        "maxLength": 55,
        "examples": ["foo123"]
      },
      "firstname": {
        "$id": "/properties/firstname",
        "type": "string",
        "title": "The First_name Schema ",
        "default": "",
        "minLength": 1,
        "maxLength": 55,
        "examples": ["Michael"]
      },
      "lastname": {
        "$id": "/properties/lastname",
        "type": "string",
        "title": "The Last_name Schema ",
        "default": "",
        "minLength": 1,
        "maxLength": 55,
        "examples": ["Hueter"]
      },
      "email": {
        "$id": "/properties/email",
        "type": "string",
        "minLength": 6,
        "maxLength": 60,
        "title": "The Email Schema ",
        "default": "",
        "examples": ["michael@rithmschool.com"]
      },
      "photo": {
        "$id": "/properties/photo",
        "type": "file",
        "title": "The Photo Schema ",
        "default": ""
      }
    },
    "required": ["username", "password"]
  }
  